    #x = @sprites.x + @cx+size.width
    #y = @sprites.y + @cy

    dx = -@map.display_x/4
    dy = -@map.display_y/4
    sprk.center(x+3-dx,y+3-dy)
    sprk.blend_type = 1
    @sparks.push(sprk)

  end

  #--------------------------------------------------------------------------
