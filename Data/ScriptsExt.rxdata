	end

	def press?(key)
	  if !@keys.has_key?(key) && state?(key)
	  	@keys[key] = Graphics.frame_count
	  	return true
	  else
	  	return false
	  end
	end 

  def hold?(key)
    return true if press?(key)
    if @keys.has_key?(key)
      if (Graphics.frame_count - @keys[key]) % 8 == 7
        return true
      end
    end
    return false
  end 

  def down?(key)
    return state?(key)
  end

  def up?(key)
    return !state?(key)
  end

