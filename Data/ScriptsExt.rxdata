  #--------------------------------------------------------------------------
  def refresh_tileset
    
    @tilemap.refresh(@map)

    @character_sprites.each{ |s| s.dispose }
    @character_sprites = []

    @map.events.keys.sort.each{ |i|
      sprite = Sprite_Character.new(@vp_main, @map.events[i])
      @character_sprites.push(sprite)
    }

    @character_sprites.push(Sprite_Character.new(@vp_main, @player))

  end

