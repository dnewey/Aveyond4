      self.opacity += 20
      @state = :up if self.opacity >= 255
    elsif @state == :up
      @life -= 1
      @state = :out if @life <= 0
    elsif @state == :out
      self.opacity -= 20
      @state = :done if self.opacity <= 0
    end
    
    #p @state

  end

